<p id="notice"><%= notice %></p>

<h1>To Arduinos</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @to_arduinos.each do |to_arduino| %>
      <tr>
        <td><%= to_arduino.value %></td>
        <td><%= link_to 'Show', to_arduino %></td>
        <td><%= link_to 'Edit', edit_to_arduino_path(to_arduino) %></td>
        <td><%= link_to 'Destroy', to_arduino, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

