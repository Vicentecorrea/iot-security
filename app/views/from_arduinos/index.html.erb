<p id="notice"><%= notice %></p>

<h1>From Arduinos</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @from_arduinos.each do |from_arduino| %>
      <tr>
        <td><%= from_arduino.value %></td>
        <td><%= link_to 'Show', from_arduino %></td>
        <td><%= link_to 'Edit', edit_from_arduino_path(from_arduino) %></td>
        <td><%= link_to 'Destroy', from_arduino, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New From Arduino', new_from_arduino_path %>
